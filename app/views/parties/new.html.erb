<h1>Create a new Party</h1>
<%= form_with (model: @party, local: true) do |f| %>
  <%= f.label :name, "Party Name" %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :description, "Description" %>
  <%= f.text_field :description %>
  <br>
  <%= f.label :location, "Location" %>
  <%= f.text_field :location %>
  <br>
  <%= f.label :date_time, "Date & Time" %>
  <%= f. %>
  <br>
  <%= f.label :dress_code, "Dress Code" %>
  <%= f.text_field :dress_code %>
  
  <%= f.summit %>
  <% end %>

