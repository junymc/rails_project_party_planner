<h1>New Account</h1>

<%= form_with(model: @account) do |f| %>
<div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password_digest %>
  </div>

    <%= f.submit %>
  <% end %>
